(Type uint32_t "#include <inttypes.h>")

(Event TokenCountResult

       ;; ID, based on the creation ID issued to a read token, or
       ;; zero if the count refers to the write token/entry
       (uint32_t clientid)

       ;; list of counts, for reading or writing
       (uint32_t count)
       )
