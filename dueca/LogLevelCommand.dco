(Type LogLevel "#include <LogLevel.hxx>")
(Type int32_t)
(Type LogCategory "#include <LogCategory.hxx>")

(Event LogLevelCommand
       (IncludeFile LogLevelCommandExtra)

       ;; Node in which the command is to be changed
       (int32_t node)
       ;; New level
       (LogLevel level)
       ;; Which logging category is to follow that level
       (LogCategory category)
 )
