<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0" />
  <object class="GtkAdjustment" id="adjustment1">
    <property name="upper">100</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkAdjustment" id="adjustment2">
    <property name="upper">100</property>
    <property name="step-increment">1</property>
    <property name="page-increment">10</property>
  </object>
  <object class="GtkWindow" id="net_use_view">
    <property name="can-focus">0</property>
    <property name="title" translatable="1">Network Use View</property>
    <property name="child">
      <object class="GtkBox">
        <property name="can-focus">0</property>
        <property name="vexpand">1</property>
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkBox">
            <style>
              <class name="toolbar" />
            </style>
            <property name="can-focus">0</property>
            <property name="valign">start</property>
            <child>
              <object class="GtkButton" id="close">
                <property name="can-focus">0</property>
                <property name="label" translatable="1">_Close</property>
                <property name="use-underline">1</property>
                <property name="icon-name">window-close</property>
              </object>
            </child>
            <child>
              <object class="GtkSeparator" />
            </child>
            <child>
              <object class="GtkLabel" id="maxtime_label">
                <property name="tooltip-text" translatable="1">
                  Maximum cycle duration of last
                  recording period.
                </property>
                <property name="name">maxtime_label</property>
                <property name="can-focus">0</property>
                <property name="label" translatable="1">0 [&amp;#956;s]</property>
                <property name="use-markup">1</property>
              </object>
            </child>
            <child>
              <object class="GtkSeparator" />
            </child>
            <child>
              <object class="GtkLabel" id="sutlabel">
                <property name="tooltip-text" translatable="1">
                  Cycle time estimation; set-up
                  overhead and time per byte
                </property>
                <property name="name">sutlabel</property>
                <property name="can-focus">0</property>
                <property name="label" translatable="1">0.0 [&amp;#956;s] + 0.0 [&amp;#956;s/byte]</property>
                <property name="use-markup">1</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkScrolledWindow">
            <property name="hadjustment">adjustment1</property>
            <property name="vadjustment">adjustment2</property>
            <property name="propagate-natural-width">1</property>
            <property name="propagate-natural-height">1</property>
            <property name="child">
              <object class="GtkViewport">
                <property name="can-focus">0</property>
                <property name="vexpand">1</property>
                <property name="child">
                  <object class="GtkBox" id="graph_box">
                    <property name="can-focus">0</property>
                    <property name="orientation">vertical</property>
                    <child>
                      <object class="GtkDrawingArea" id="timing_view">
                        <property name="name">timing_view</property>
                        <property name="width-request">220</property>
                        <property name="can-focus">0</property>
                        <property name="tooltip-markup" translatable="1">&lt;big&gt;Histogram of
                          communication cycle timing.&lt;/big&gt;
                          20-point histogram. The red line indicates when cycle time is exceeded.</property>
                        <property name="halign">start</property>
                      </object>
                    </child>
                    <child>
                      <placeholder />
                    </child>
                    <child>
                      <placeholder />
                    </child>
                  </object>
                </property>
              </object>
            </property>
          </object>
        </child>
      </object>
    </property>
  </object>
</interface>