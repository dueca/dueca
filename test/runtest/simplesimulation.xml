    <click window="HID GUI" x="29" y="99" button="Button.left" pressed="false" wait="0.5"/>
    <snap name="simplesimulation-run.png"/>
    <check window="DUECA/DUSIME control panel" x="479" y="236" r="104" g="104" b="104" timeout="10.0" wait="10.5"/>
    <click window="DUECA/DUSIME control panel" x="479" y="236" button="Button.left" pressed="true" wait="0.2"/>
    <click window="DUECA/DUSIME control panel" x="479" y="236" button="Button.left" pressed="false" wait="0.2"/>
