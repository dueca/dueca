<?xml version='1.0' encoding='UTF-8'?>
<scenario xmlns="https://dueca.tudelft.nl"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="https://dueca.tudelft.nl https://dueca.tudelft.nl/schemas/testscenario.xsd">
  <project name="SimpleSimulation">
    <window>DUECA/DUSIME control panel</window>
    <window>Are you serious?</window>
    <window>HID GUI</window>
    <window>front</window>
  </project>
  <repository>https://github.com/dueca/</repository>
  <execute>
    <platform>simlab</platform>
    <node>host</node>
  </execute>
  <execute>
    <platform>simlab</platform>
    <node>igtest</node>
  </execute>
  <actions>
