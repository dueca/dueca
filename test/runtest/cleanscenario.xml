<?xml version='1.0' encoding='UTF-8'?>
<scenario xmlns="https://dueca.tudelft.nl" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://dueca.tudelft.nl testscenario.xsd">

  <!-- set the proper project name here. The project will be checked
       out and configured and compiled -->
  <project name="FlexiStick">
    <!-- The project will be filled with a list of windows that were
	 used. Each window needs to have a unique name -->
  </project>

  <!-- specify the repository -->
  <repository>git@github.com:dueca/</repository>

  <!-- Each execute given here starts a DUECA process.
       
       Supply platform and node, you can use multiple 'execute' tags,
       so you can start multi-node DUECA processes. -->
  <execute>
    <platform>solo</platform>
    <node>solo</node>
  </execute>
  
  <!-- Keep 'actions' clean, this will be filled by the script. You
       can later edit and correct actions if you want. -->
  <actions>
  </actions>
</scenario>
